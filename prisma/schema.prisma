generator client {
  provider        = "prisma-client-js"
  previewFeatures = ["referentialIntegrity"]
}

datasource db {
  provider             = "mysql"
  url                  = env("DATABASE_URL")
  referentialIntegrity = "prisma"
}

model Emails {
  id        Int      @id @default(autoincrement())
  email     String
  group     String
  createdAt DateTime @default(now())
}
